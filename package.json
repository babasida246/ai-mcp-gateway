{
    "name": "mcp-gateway",
    "version": "0.1.0",
    "description": "AI MCP Gateway - Cost-Optimized Multi-Model Orchestrator for Code & Tools",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": {
        "mcp-gateway": "./dist/index.js"
    },
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "start": "node dist/index.js",
        "start:api": "MODE=api node dist/index.js",
        "start:mcp": "node dist/index.js",
        "db:migrate": "tsx scripts/run-migrations.ts",
        "setup": "node quick-start.js",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:ui": "vitest --ui",
        "test:e2e": "playwright test",
        "test:e2e:ui": "playwright test --ui",
        "test:e2e:debug": "playwright test --debug",
        "test:e2e:headed": "playwright test --headed",
        "type-check": "tsc --noEmit",
        "lint": "eslint src --ext .ts",
        "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\""
    },
    "keywords": [
        "mcp",
        "model-context-protocol",
        "ai",
        "llm",
        "orchestrator",
        "multi-model",
        "cost-optimization",
        "code-agent"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@anthropic-ai/sdk": "^0.27.3",
        "@modelcontextprotocol/sdk": "^0.5.0",
        "@openrouter/sdk": "^0.1.27",
        "@types/express": "^5.0.5",
        "@types/ssh2": "^1.15.5",
        "axios": "^1.13.2",
        "bcryptjs": "^3.0.3",
        "dotenv": "^16.6.1",
        "express": "^5.1.0",
        "ioredis": "^5.8.2",
        "jsonwebtoken": "^9.0.3",
        "openai": "^4.67.3",
        "pg": "^8.16.3",
        "ssh2": "^1.17.0",
        "uuid": "^13.0.0",
        "winston": "^3.14.2",
        "zod": "^3.25.76",
        "ws": "^8.13.0"
    },
    "devDependencies": {
        "@playwright/test": "^1.48.0",
        "@types/bcryptjs": "^2.4.6",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/node": "^20.19.25",
        "@types/pg": "^8.15.6",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^8.0.0",
        "@typescript-eslint/parser": "^8.0.0",
        "@vitest/ui": "^2.1.1",
        "eslint": "^9.9.0",
        "prettier": "^3.3.3",
        "tsup": "^8.2.3",
        "tsx": "^4.21.0",
        "typescript": "^5.5.3",
        "vitest": "^2.1.1",
        "supertest": "^6.3.3"
    },
    "engines": {
        "node": ">=20.0.0"
    }
}